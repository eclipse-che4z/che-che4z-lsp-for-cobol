    "number-constant" : {
      "match" : "(?<![\\+\\-\\w])(\\+|\\-)?\\d+(\\.\\d+)?([Ee](\\-|\\+)\\d+)?(?![\\-\\w])",
      "name" : "constant.numeric.integer"
    },
    "string-quoted-constant" : {
      "begin" : "'",
      "end" : "'|$",
      "name" : "string.quoted.single.cobol"
    },
    "string-double-quoted-constant" : {
      "begin" : "\"",
      "end" : "\"|$",
      "name" : "string.quoted.double.cobol"
    },
    "picture" : {
      "match" : "(?i:\\b(picture|pic)\\s+(is\\s+)?\\S+)(?<!\\.)",
