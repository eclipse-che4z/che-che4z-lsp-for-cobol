    "@shelex/cypress-allure-plugin": "^2.5.1",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "allure-commandline": "^2.13.8",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.3",
    "cypress-iframe": "^1.0.1",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-tags": "^0.2.0",
    "cypress-terminal-report": "^3.2.2",
    "cypress-xpath": "^1.6.1",
    "debug": "^4.3.2",
    "eslint": "^7.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.5",
    "junit-report-merger": "^2.2.3",
    "lint-staged": "^10.5.3",
    "mocha-junit-reporter": "^2.0.0",
    "prettier": "^2.0.5",
    "ts-loader": "^8.0.6",
    "webpack": "^5.44.0"
  },
  "dependencies": {
    "@eclipse/che-che4z": "https://github.com/eclipse/che-che4z.git",
    "cypress": "7.7.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "lint": "eslint . ",
    "cy:run": "npm run allure:clear && cross-env CYPRESS_EXCLUDE_TAGS=flaky,flaky_theia,bug,investigation cypress run --env allure=true  -b chrome",
    "cy:run:all": "npm run cy:run -- --spec cypress/integration/LSP/*",
    "cy:run:all:headless": "npm run cy:run:all  -- --headless",
    "cy:open": "cypress open --env allure=true",
    "cy:run:all:headless:smoke": "cross-env CYPRESS_INCLUDE_TAGS=smoke npm run cy:run:all:headless --",
    "ts:build": "tsc --build ./node_modules/@eclipse/che-che4z/tests/tsconfig.json",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open",
    "allure:open:report": "npm run allure:report && allure open",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "delete:reports": "rm cypress/results/* || true",
    "prereport": "npm run delete:reports",
    "merge-reports": "jrm ui_tests_complete_logs.xml \"results/*.xml\"",
    "cy:run:ci": "npm run prereport && cross-env CYPRESS_INCLUDE_TAGS=CI cypress run -b chrome --headless --reporter mocha-junit-reporter --config defaultCommandTimeout=100000 --env appLoadTimeout=100000"
  }
}
